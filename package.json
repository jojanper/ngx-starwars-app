{
  "name": "ngx-starwars-app",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jojanper/ngx-starwars-app.git"
  },
  "bugs": {
    "url": "https://github.com/jojanper/ngx-starwars-app/issues"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "watch-prod": "webpack --watch --progress -p",
    "webpack-profile": "webpack --profile --json > webpack-stats.json",
    "build": "rimraf dist && webpack --progress --profile --bail -p",
    "server": "webpack-dev-server --inline --progress --port 3002",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --project tslint.json \"src/**/*.ts\"",
    "test": "node --max-old-space-size=8192 ./node_modules/karma/bin/karma start",
    "posttest": "npm run coverage_text && npm run coverage_text_summary && cat coverage/text.txt && cat coverage/text-summary.txt",
    "test-watch": "node --max-old-space-size=8192 ./node_modules/karma/bin/karma start --no-single-run --auto-watch",
    "cibuild-travis": "npm run lint && npm test -- --travis",
    "start": "npm run server",
    "start:hmr": "npm run server -- --hot",
    "postinstall": "npm run webdriver-update",
    "coverage_html": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "coverage_text": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/text.txt -t text",
    "coverage_lcov": "remap-istanbul -i coverage/json/coverage-final.json -o coverage.lcov -t lcovonly",
    "coverage_text_summary": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/text-summary.txt -t text-summary"
  },
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.0.34",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "angular-router-loader": "0.8.0",
    "core-js": "^2.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@types/chosen-js": "^1.6.2",
    "@types/jasmine": "^2.8.2",
    "@types/jquery": "^3.2.16",
    "@types/node": "^8.0.53",
    "@types/selenium-webdriver": "3.0.8",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^7.1.6",
    "awesome-typescript-loader": "^3.4.0",
    "codecov": "^3.0.0",
    "codelyzer": "^4.0.1",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.6",
    "node-sass": "^4.7.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.0.9",
    "protractor": "^5.2.0",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "style-loader": "^0.19.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "engines": {
    "node": ">= 8.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
